{
	"name": "timeout",
	"displayName": "Timeout",
	"description": "",
	"version": "0.0.4",
	"repository": "https://bitbucket.org/SamBueth/timeout",
	"engines": {
		"vscode": "^1.30.0"
	},
	"publisher": "sBueth",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
				"command": "timeout.start",
				"title": "Timeout starten"
			},
			{
				"command": "timeout.set",
				"title": "Timeout setzen"
			},
			{
				"command": "timeout.startlast",
				"title": "Timeout fortsetzten"
			},
			{
				"command": "timeout.stop",
				"title": "Timeout stoppen"
			}
		],
		"configuration": [{
			"title": "Worktimeout",
			"properties": {
				"timeout.autostart": {
					"type": "boolean",
					"default": true
				},
				"timeout.worktime": {
					"type": "number",
					"default": 7.6
				},
				"timeout.breaktime": {
					"type": "number",
					"default": 0.5
				}
			}
		}]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"typescript-dotnet-umd": "^4.11.2",
		"version": "^0.1.2"
	}
}
